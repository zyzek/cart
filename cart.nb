Cell[TextData[{
 "Each CA rule is a function from bit-triples to bits; ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["b", "3"], "\[Rule]", "b"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9eb862dd-2b8f-4d73-a3fc-0333bdb5a3a9"],
